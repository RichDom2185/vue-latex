{
  "name": "@richdom2185/vue-latex",
  "version": "0.1.2",
  "type": "module",
  "description": "Render LaTeX in Vue components.",
  "author": "Richard Dominick",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
