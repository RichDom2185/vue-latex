  "dependencies": {
    "katex": "^0.16.10"
  },
  "devDependencies": {
    "@types/katex": "^0.16.7",
    "eslint": "^9.9.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vue": "^3.4.38"
  },
  "homepage": "https://github.com/RichDom2185/vue-latex#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RichDom2185/vue-latex.git"
  },
  "bugs": {
    "url": "https://github.com/RichDom2185/vue-latex/issues"
  }
